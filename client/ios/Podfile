platform :ios, '11.0'

workspace 'app.xcworkspace'

target 'iDEAL Example (Swift)' do
  project 'iDEAL Example (Swift).xcodeproj'

  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Sample
  pod 'Stripe'
end

